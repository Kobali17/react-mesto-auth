{
  "extends": ["airbnb-base",
    "plugin:react/recommended"
  ],
  "globals": {
    "localStorage": true,
    "fetch":true,
    "console": true,
    "document": true
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "react/prop-types": 0,
    "no-underscore-dangle": [
      2,
      {
        "allow": [
          "_id",
          "_fetch",
          "_headers",
          "_baseUrl"

        ]
      }
    ]
  },
  "plugins": ["react"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  }

}